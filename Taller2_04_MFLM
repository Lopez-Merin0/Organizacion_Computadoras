;Direccionamiento registro indirecto

section .data
numero1 dd 5         
numero2 dd 7         
resultado dd 0    

section .text
global _start       

_start:
    mov ebx, numero1   ; Carga la direcci贸n de memoria de "numero1" en ebx
    mov eax, [ebx]     ; Mueve el valor apuntado por ebx (valor de "numero1") a eax
    add ebx, 4         ; Avanza el puntero para apuntar a "numero2" 
    add eax, [ebx]     ; Suma el valor apuntado por ebx (valor de "numero2") a eax
    mov ebx, resultado ; Carga la direcci贸n de memoria de "resultado" en ebx
    mov [ebx], eax     ; Guarda el resultado en la direcci贸n de memoria de "resultado"
    
    mov eax, 1         ; C贸digo de salida del programa (sys_exit)
    xor ebx, ebx       ; Codigo de retorno 0
    int 0x80           ; Llamada de salida
