; 2 Valores de 8 bits, operacion AND, almacenamiento en el tercero

section .data
    val1: .byte 0xAA
    val2: .byte 0x55
    result: .byte 0x00

section .text
    global _start

_start:
    mov val1, %al       
    mov val2, %bl      
    and %bl, %al        ; Realiza la operaci√≥n entre val1 y val2
    mov %al, result     ; Almacena el resultado

    mov $1, %eax         ; Codigo de Salida
    int 0x80             ; Salir
